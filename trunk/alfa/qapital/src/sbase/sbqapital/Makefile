.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET) 

staticlib: $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC) $(OBJCOMP) 
	-$(DEL_FILE) $(TARGET)
	$(AR) $(TARGET) $(OBJECTS) $(OBJMOC)


mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: sbqapital.pro  /usr/qt/3/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) -o Makefile sbqapital.pro
qmake: 
	@$(QMAKE) -o Makefile sbqapital.pro

dist: 
	@mkdir -p .obj/sbqapital && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/sbqapital/ && ( cd `dirname .obj/sbqapital` && $(TAR) sbqapital.tar sbqapital && $(GZIP) sbqapital.tar ) && $(MOVE) `dirname .obj/sbqapital`/sbqapital.tar.gz . && $(DEL_FILE) -r .obj/sbqapital

mocclean:

uiclean:

yaccclean:
lexclean:
clean:
