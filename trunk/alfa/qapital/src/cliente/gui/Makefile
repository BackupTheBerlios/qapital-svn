MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-mdibased \
		sub-menubased

first: all

all: Makefile $(SUBTARGETS)

mdibased/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "mdibased" || $(MKDIR) "mdibased"
	cd mdibased && $(QMAKE) mdibased.pro -o $(MAKEFILE)
sub-mdibased: mdibased/$(MAKEFILE) FORCE
	cd mdibased && $(MAKE) -f $(MAKEFILE)

menubased/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "menubased" || $(MKDIR) "menubased"
	cd menubased && $(QMAKE) menubased.pro -o $(MAKEFILE)
sub-menubased: menubased/$(MAKEFILE) FORCE
	cd menubased && $(MAKE) -f $(MAKEFILE)

Makefile: gui.pro  /usr/qt/3/mkspecs/linux-g++/qmake.conf 
	$(QMAKE) -o Makefile gui.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile gui.pro

all: $(SUBTARGETS)
qmake_all: mdibased/$(MAKEFILE) menubased/$(MAKEFILE)
	 ( [ -d mdibased ] && cd mdibased ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d menubased ] && cd menubased ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d mdibased ] && cd mdibased ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d menubased ] && cd menubased ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d mdibased ] && cd mdibased ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d menubased ] && cd menubased ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d mdibased ] && cd mdibased ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d menubased ] && cd menubased ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d mdibased ] && cd mdibased ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d menubased ] && cd menubased ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

