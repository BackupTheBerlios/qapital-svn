MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-qpadmin \
		sub-qpcliente

first: all

all: Makefile $(SUBTARGETS)

qpadmin/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "qpadmin" || $(MKDIR) "qpadmin"
	cd qpadmin && $(QMAKE) qpadmin.pro -o $(MAKEFILE)
sub-qpadmin: qpadmin/$(MAKEFILE) FORCE
	cd qpadmin && $(MAKE) -f $(MAKEFILE)

qpcliente/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "qpcliente" || $(MKDIR) "qpcliente"
	cd qpcliente && $(QMAKE) qpcliente.pro -o $(MAKEFILE)
sub-qpcliente: qpcliente/$(MAKEFILE) FORCE
	cd qpcliente && $(MAKE) -f $(MAKEFILE)

Makefile: src.pro  /usr/share/qt3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile src.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile src.pro

all: $(SUBTARGETS)
qmake_all: qpadmin/$(MAKEFILE) qpcliente/$(MAKEFILE)
	 ( [ -d qpadmin ] && cd qpadmin ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d qpcliente ] && cd qpcliente ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d qpadmin ] && cd qpadmin ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d qpcliente ] && cd qpcliente ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d qpadmin ] && cd qpadmin ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d qpcliente ] && cd qpcliente ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d qpadmin ] && cd qpadmin ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d qpcliente ] && cd qpcliente ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d qpadmin ] && cd qpadmin ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d qpcliente ] && cd qpcliente ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

