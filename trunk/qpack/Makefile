#############################################################################
# Makefile for building: qpack
# Generated by qmake (1.07a) (Qt 3.3.3) on: Fri Feb 25 15:56:32 2005
# Project:  qpack.pro
# Template: subdirs
# Command: $(QMAKE) -o Makefile qpack.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-sbase \
		sub-src

first: all

all: Makefile $(SUBTARGETS)

sbase/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "sbase" || $(MKDIR) "sbase"
	cd sbase && $(QMAKE) sbase.pro -o $(MAKEFILE)
sub-sbase: sbase/$(MAKEFILE) FORCE
	cd sbase && $(MAKE) -f $(MAKEFILE)

src/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "src" || $(MKDIR) "src"
	cd src && $(QMAKE) src.pro -o $(MAKEFILE)
sub-src: src/$(MAKEFILE) FORCE
	cd src && $(MAKE) -f $(MAKEFILE)

Makefile: qpack.pro  /usr/share/qt3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile qpack.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile qpack.pro

all: $(SUBTARGETS)
qmake_all: sbase/$(MAKEFILE) src/$(MAKEFILE)
	 ( [ -d sbase ] && cd sbase ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d src ] && cd src ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d sbase ] && cd sbase ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d sbase ] && cd sbase ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d sbase ] && cd sbase ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d sbase ] && cd sbase ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d src ] && cd src ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

